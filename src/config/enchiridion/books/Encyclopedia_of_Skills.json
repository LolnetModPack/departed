        },
        {
          "type": "FeatureText",
          "properties": {
            "text": "Heartstone Healing Levels:\n    \n    Level 15: 1 Heart\n    Level 30: 2 Hearts\n    Level 45: 3 Hearts\n    Level 60: 4 Hearts\n    Level 75: 5 Hearts\n    Level 90: 7 Hearts\n    Level 99: 10 Hearts\n\nDodge/Regen Chance:\n\n    Level 15: 3.3%\n    Level 30: 5%\n    Level 50: 10%\n    Level 70: 14%\n    Level 90: 25%",
            "wrap": 0,
